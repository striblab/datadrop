{
  "name": "mapzen.js",
  "version": "0.1.0",
  "description": "Javascript SDK for Mapzen products",
  "main": "dist/mapzen.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start test/karma.conf.js --log-level debug --single-run",
    "lint": "eslint ./src/js/* ./test/*.js; exit 0;",
    "build-style": "node-sass --output-style compressed -o dist/ src/scss/mapzen.scss; cp -R node_modules/mapzen-scarab/src/images dist/images; cp -r node_modules/leaflet/dist/images/* dist/images/;",
    "build-dev-style": "node-sass --output-style compressed -o dist/ src/scss/mapzen.scss -w; cp -R node_modules/mapzen-scarab/src/images dist/images; cp -r node_modules/leaflet/dist/images/* dist/images/;",
    "dev": " cp src/dev.html dist/index.html; watchify  ./src/js/mapzen.js -o dist/mapzen.min.js -v;",
    "build": "browserify ./src/js/mapzen.js > dist/mapzen.js; browserify -t uglifyify ./src/js/mapzen.js | uglifyjs -c > dist/mapzen.min.js; cp src/index.html dist/; npm run-script build-style;"
  },
  "dependencies": {

  },
  "devDependencies": {
    "benchmark": "^2.1.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "eslint": "^2.8.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "expect.js": "^0.3.1",
    "happen": "^0.3.1",
    "istanbul": "^0.4.3",
    "karma": "^0.13.22",
    "karma-browserstack-launcher": "^0.1.11",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-requirejs": "^0.2.6",
    "leaflet": "^0.7.7",
    "leaflet-geocoder-mapzen": "^1.6.2",
    "leaflet-hash": "^0.2.1",
    "leaflet.locatecontrol": "^0.52.0",
    "mapzen-scarab": "git+https://github.com/mapzen/scarab.git",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "requirejs": "^2.2.0",
    "sinon": "^1.17.3",
    "uglifyify": "^3.0.1",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapzen/mapzen.js.git"
  },
  "author": "MAPZEN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapzen/mapzen.js/issues"
  },
  "homepage": "https://github.com/mapzen/mapzen.js",
  "keywords": [
    "map",
    "mapzen",
    "eraser map",
    "map"
  ]
}
